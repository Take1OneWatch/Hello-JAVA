import java.awt.*;
import java.awt.event.*;
import java.io.*;
import javax.swing.*;
public class test104 extends JFrame implements ActionListener{
 String filename="e:\\实验\\test104.txt";//这里写你准备好的文本文件的全路径名
 JTextArea jta=new JTextArea(5,3);
 JScrollPane jsp=new JScrollPane(jta);
 JButton jb=new JButton("确定");
 JPanel jp=new JPanel();
 test104(){
 setTitle("教师信息");
 jb.addActionListener(this);
 jp.add(jb);
 add(jsp,BorderLayout.CENTER);
 add(jp,BorderLayout.SOUTH);
 setSize(300, 300);
 setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
  setLocationRelativeTo(null);
  readFile(filename);
  setVisible(true);
 }
 public static void main(String[] args) {
  new test101();
 }

 public void actionPerformed(ActionEvent e) {
  if(e.getSource()==jb){
	   setVisible(false);// 本窗口隐藏,
       new Test1().setVisible(true);// 新窗口显示
       dispose();//本窗口销毁,释放内存资源
  }
 }
 public void readFile(String filename2){
  FileReader fr = null;
  BufferedReader br = null;
  try {
   fr= new FileReader(filename2);
   br=new BufferedReader(fr);
   String str;
   while((str=br.readLine())!=null){
     jta.append(str+"\n");
   }
  } catch (FileNotFoundException e) {
   e.printStackTrace();
  } catch (IOException e) {
   e.printStackTrace();
  } finally{
   try {
    br.close();
    fr.close();
   } catch (IOException e) {
    e.printStackTrace();
   }
  }
 }
}








import java.awt.*;
import java.awt.event.*;
import java.io.*;
import javax.swing.*;
public class test105 extends JFrame implements ActionListener{
 String filename="e:\\实验\\test105.txt";//这里写你准备好的文本文件的全路径名
 JTextArea jta=new JTextArea(5,3);
 JScrollPane jsp=new JScrollPane(jta);
 JButton jb=new JButton("确定");
 JPanel jp=new JPanel();
 test105(){
 setTitle("教师信息");
 jb.addActionListener(this);
 jp.add(jb);
 add(jsp,BorderLayout.CENTER);
 add(jp,BorderLayout.SOUTH);
 setSize(300, 300);
 setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
  setLocationRelativeTo(null);
  readFile(filename);
  setVisible(true);
 }
 public static void main(String[] args) {
  new test101();
 }

 public void actionPerformed(ActionEvent e) {
  if(e.getSource()==jb){
	   setVisible(false);// 本窗口隐藏,
       new Test1().setVisible(true);// 新窗口显示
       dispose();//本窗口销毁,释放内存资源
  }
 }
 public void readFile(String filename2){
  FileReader fr = null;
  BufferedReader br = null;
  try {
   fr= new FileReader(filename2);
   br=new BufferedReader(fr);
   String str;
   while((str=br.readLine())!=null){
     jta.append(str+"\n");
   }
  } catch (FileNotFoundException e) {
   e.printStackTrace();
  } catch (IOException e) {
   e.printStackTrace();
  } finally{
   try {
    br.close();
    fr.close();
   } catch (IOException e) {
    e.printStackTrace();
   }
  }
 }
}









import java.awt.*;
import java.awt.event.*;
import java.io.*;
import javax.swing.*;

public class Test7 extends JFrame implements ActionListener {
	 JButton jb1, jb2,jb3;//按钮
	    JLabel jl1,jl2,jl3,jl4,jl5,jl6,jl7,jl8,jl9,jl10;
	    JTextField write,write2,write3,write4,write5,write6,write7,write8;//文本框
	   
	  
	    public Test7(){
	    	setTitle("");//标题
		    setSize(500, 500);//大小
		    setLocationRelativeTo(null);//居中
		    JPanel jp = new JPanel();
		    jp.setLayout(new GridLayout(10, 1));
		    
		    
		    JPanel jp1 = new JPanel();
		    jl3=new JLabel("请在下面输入教师的信息！");
		    jp1.add(jl3);
		    jp.add(jp1);
		    
		    
		    JPanel jp2 = new JPanel();
		    jl1=new JLabel("工号 :");
		    jp2.add(jl1);
		    write = new JTextField(12);
		   
		    jp2.add(write);
		    jp.add(jp2);
		    
		    
		    JPanel jp6 = new JPanel();
		    jl3=new JLabel("年龄 :");
		    jp6.add(jl3);
		    write3 = new JTextField(12);
		    jp6.add(write3);
		    jp.add(jp6);
		    
		    
		    
		    
		    JPanel jp3 = new JPanel();
		    jl2=new JLabel("职称 :");
		    jp3.add(jl2);
		    write8 = new JTextField(12);
		    jp3.add(write8);
		    jp.add(jp3);
		    
		
		    
		    JPanel jp7 = new JPanel();
		    jl4=new JLabel("所在系:");
		    jp7.add(jl4);
		    write4 = new JTextField(12);
		    jp7.add(write4);
		    jp.add(jp7);
		    
		    
		    JPanel jp8 = new JPanel();
		    jl5=new JLabel("教授的主要课程:");
		    jp8.add(jl5);
		    write5 = new JTextField(12);
		    jp8.add(write5);
		    jp.add(jp8);
		    
		    
		    JPanel jp9 = new JPanel();
		    jl6=new JLabel("联系电话:");
		    jp9.add(jl6);
		    write6 = new JTextField(12);
		    jp9.add(write6);
		    jp.add(jp9);
		    
		    
		    
		    JPanel jp10 = new JPanel();
		    jl7=new JLabel("联系地址:");
		    jp10.add(jl7);
		    write7 = new JTextField(12);
		    jp10.add(write7);
		    jp.add(jp10);
		    
		    
		
		    
		    
		    JPanel jp4 = new JPanel();
		    jb1=new JButton("增加");
		    jb3 = new JButton("取消");
		    jb3.addActionListener(this);// 添加动作响应器
		    jb1.addActionListener(this);// 添加动作响应器
		    jp4.add(jb1);
		    jp4.add(jb3);
		    jp.add(jp4);
		    
		    
		    
		    JPanel jp5 = new JPanel();
		    jl4=new JLabel("");
		    jp5.add(jl4);
		    jp.add(jp5);
		    add(jp);   
		    
		    
		    
	    }
	     
	
	
	 public void actionPerformed(ActionEvent e) {
		 String cmd = e.getActionCommand();
		 if (cmd.equals("取消")) {
			 setVisible(false);
             new Test1().setVisible(true);
             dispose();
			 
		 }
		 
		 if(e.getSource()==jb1){
			   if(write.getText().equals("")){
			    JOptionPane.showMessageDialog(null,"请输入工号~","错误",JOptionPane.ERROR_MESSAGE);
			    write.grabFocus();
			   }else{
			    write1(write.getText());
			    JOptionPane.showMessageDialog(null,"写入成功","提示",JOptionPane.INFORMATION_MESSAGE);
			    setVisible(false);// 本窗口隐藏,
                new Test13().setVisible(true);// 新窗口显示
                dispose();//本窗口销毁,释放内存资源
			   }
			  }
		 if(e.getSource()==jb1){
			   if(write3.getText().equals("")){
			    JOptionPane.showMessageDialog(null,"请输入年龄~","错误",JOptionPane.ERROR_MESSAGE);
			    write3.grabFocus();
			   }else{
			    write2(write3.getText());
			  
			   }
			  }
		 
		 if(e.getSource()==jb1){
			   if(write8.getText().equals("")){
			    JOptionPane.showMessageDialog(null,"请输入职称~","错误",JOptionPane.ERROR_MESSAGE);
			    write8.grabFocus();
			   }else{
			    write3(write8.getText());
			   }
			  }
		 
		 if(e.getSource()==jb1){
			   if(write4.getText().equals("")){
			    JOptionPane.showMessageDialog(null,"请输入所在系~","错误",JOptionPane.ERROR_MESSAGE);
			    write4.grabFocus();
			   }else{
			    write4(write4.getText());
			  
			   }
			  }
		 if(e.getSource()==jb1){
			   if(write5.getText().equals("")){
			    JOptionPane.showMessageDialog(null,"请输入教授的主要课程~","错误",JOptionPane.ERROR_MESSAGE);
			    write5.grabFocus();
			   }else{
			    write5(write5.getText());
			  
			   }
			  }
		 if(e.getSource()==jb1){
			   if(write6.getText().equals("")){
			    JOptionPane.showMessageDialog(null,"请输入联系电话~","错误",JOptionPane.ERROR_MESSAGE);
			    write6.grabFocus();
			   }else{
			    write6(write6.getText());
			  
			   }
			  }
		 if(e.getSource()==jb1){
			   if(write7.getText().equals("")){
			    JOptionPane.showMessageDialog(null,"请输入联系地址~","错误",JOptionPane.ERROR_MESSAGE);
			    write7.grabFocus();
			   }else{
			    write7(write7.getText());
			  
			   }
			  }
		
	 }
	 
	 
