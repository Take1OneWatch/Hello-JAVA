		   File f=new File("e:\\实验\\test103.txt");//向指定文本框内写入
		   FileWriter fw=new FileWriter(f,true);
		   fw.write(line);
		   fw.write("\r\n");
		   fw.close();
		  }catch(Exception e){
		 
		  }
		 }
	 
